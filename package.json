{
  "name": "@apagoinc/exif-js",
  "version": "3.1.0",
  "description": "JavaScript library for reading EXIF image metadata - Typescript port",
  "main": "dist/exif.js",
  "types": "dist/exif.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "prettier": "prettier -c './**/*.{ts,json,html,scss}'",
    "prettier:fix": "prettier --write './**/*.{ts,json,html,scss}'",
    "lint": "eslint src/**/*.ts --max-warnings 0",
    "lint:fix": "npm run --silent lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ApagoInc/exif-js"
  },
  "keywords": [
    "exif"
  ],
  "author": "Jacob Seidelin",
  "maintainers": [
    "rett gerst"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ApagoInc/exif-js/issues"
  },
  "homepage": "https://github.com/ApagoInc/exif-js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "spec",
    "example"
  ],
  "files": [
    "README.md",
    "LICENSE.md",
    "dist/"
  ],
  "devDependencies": {
    "prettier": "^1.18.2",
    "typescript": "^3.6.2",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-plugin-jsdoc": "^15.9.1"
  }
}
